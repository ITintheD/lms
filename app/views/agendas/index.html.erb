<h1>Agendas</h1>

<% unless @featured.blank? %>
	<div class ="hero-unit-featured">
	  <h3>Current Agenda</h3>
	  <%= render :partial => "agenda", :locals => { :agenda => @featured, :class_name => 'featured-item' } %>
	</div>
<% end %>

<br/>

<div class="list-items">
  <%= render :partial => "agenda", :collection => @agendas, :as => :agenda, :locals => { :class_name => 'list-item' } %>
</div>

<br />

